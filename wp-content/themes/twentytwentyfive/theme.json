      "margin": {
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px"
      },
      "padding": {
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px"
      }
    },
    "color": {
      "text": "var(--wp--preset--color--ar-color-neutral-05)",
      "background": "var(--wp--preset--color--ar-color-neutral-00)"
    },
    "blocks": {
      "core/pullquote": {
        "border": {
          "width": "4px",
          "radius": "10px",
          "style": "dotted",
          "color": "var(--wp--preset--color--ar-color-primary-01)"
        }
      },
      "core/paragraph": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--text-font)",
          "fontSize": "1rem",
          "fontWeight": "normal",
          "lineHeight": "1.5"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--display-font)",
          "fontSize": "2rem",
          "fontWeight": "bold"
        },
        "color": {
          "defaultPalette": true,
          "palette": [
            {
              "slug": "ar-color-primary-04",
              "color": "#4A5DAA",
              "name": "Primary 04"
            },
            {
              "slug": "ar-color-secondary-05",
              "color": "#630E35",
              "name": "Secondary 05"
            }
          ],
          "link": true
        }
      },
      "core/heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--display-font)",
          "fontSize": "2.25rem",
          "fontWeight": "bold",
          "lineHeight": "1.2"
        }
      },
      "core/preformatted": {
        "typography": {
          "fontSizes": [],
          "customFontSizes": false
        }
      }
    }
